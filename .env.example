# Environment variables for expense-tracker
# Copy this file to .env and fill in your values

# Google Places API Key (required for location functionality)
GOOGLE_PLACES_API_KEY=your_google_places_api_key_here

# Application settings
# Note: NODE_ENV=production is not supported in Vite .env files
# Only use NODE_ENV=development for development builds
PORT=3000

# Vite development server port (for frontend)
VITE_DEV_PORT=5173